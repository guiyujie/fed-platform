{
  "name": "fed-platform",
  "version": "1.0.0",
  "description": "前端管理平台",
  "main": "webpack.config.js",
  "scripts": {
    "postinstall": "npm run pack & npm run dll",
    "prebuild": "npm run clean ./dist/*",
    "build": "webpack --env production",
    "server": "node server/index.js",
    "dev": "webpack-dev-server --env development --host 0.0.0.0 --port 3000 --hot --inline --progress",
    "debug": "webpack-dev-server --env debug --host 0.0.0.0 --port 3001 --hot --inline --progress",
    "preview": "webpack-dev-server --env preview --host 0.0.0.0 --port 3002 --progress",
    "dll": "webpack --env.NODE_ENV=development --env.dll & webpack --env.NODE_ENV=production --env.dll",
    "lock": "npm shrinkwrap --dev",
    "pack": "npm run lock && shrinkpack",
    "clean": "rimraf"
  },
  "dependencies": {
    "antd": "~2.10.0",
    "axios": "~0.16.1",
    "babel-polyfill": "~6.23.0",
    "classnames": "~2.2.5",
    "jsonp": "~0.2.1",
    "qs": "~6.4.0",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-redux": "~5.0.3",
    "react-router": "~3.0.5",
    "react-router-redux": "~4.0.8",
    "redux": "~3.6.0",
    "redux-axios-middleware": "~4.0.0",
    "redux-thunk": "~2.2.0"
  },
  "devDependencies": {
    "glob": "~6.0.4",
    "rimraf": "~2.6.1",
    "shrinkpack": "~0.18.1",
    "react-transform-hmr": "~1.0.4",
    "redux-devtools": "~3.4.0",
    "redux-devtools-dock-monitor": "~1.1.2",
    "redux-devtools-log-monitor": "~1.3.0",
    "babel-core": "~6.24.0",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-import": "~1.1.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-register": "~6.24.0",
    "babel-loader": "~7.0.0",
    "babel-preset-react": "~6.23.0",
    "babel-preset-react-hmre": "~1.1.1",
    "less": "~2.7.2",
    "less-vars-to-js": "~1.1.2",
    "less-loader": "~4.0.2",
    "css-modules-require-hook": "~4.0.6",
    "css-loader": "~0.27.3",
    "postcss-loader": "1.3.3",
    "style-loader": "~0.15.0",
    "autoprefixer": "~6.7.7",
    "extract-text-webpack-plugin": "~2.1.0",
    "url-loader": "~0.5.8",
    "file-loader": "~0.10.1",
    "html-loader": "~0.4.5",
    "html-webpack-harddisk-plugin": "~0.1.0",
    "html-webpack-plugin": "~2.28.0",
    "assert": "~1.4.1",
    "assets-webpack-plugin": "~3.5.1",
    "copy-webpack-plugin": "~4.0.1",
    "webpack": "~2.3.0",
    "webpack-dev-server": "~2.4.2",
    "webpack-merge": "~4.1.0",
    "chalk": "~1.1.3",
    "chokidar": "~1.7.0",
    "express": "4.10.8",
    "express-http-proxy": "~1.0.3",
    "express-session": "1.9.1",
    "body-parser": "1.9.0",
    "cookie-parser": "1.3.3",
    "connect-mongo": "1.3.2"
  },
